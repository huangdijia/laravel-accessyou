{
    "name": "huangdijia/laravel-accessyou",
    "description": "accessyou for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "accessyou"
    ],
    "homepage": "https://github.com/huangdijia/laravel-accessyou",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^7.0",
        "illuminate/console": "^7.0",
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Huangdijia\\Accessyou\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\Accessyou\\AccessyouServiceProvider"
            ],
            "aliases": {
                "Accessyou": "Huangdijia\\Accessyou\\Facades\\Accessyou"
            }
        }
    }
}